export VULKAN_ROOT_LOCATION="/Users/lina/VulkanSDK/"
export VULKAN_SDK_VERSION="1.3.231.1"
export VULKAN_SDK="$VULKAN_ROOT_LOCATION/$VULKAN_SDK_VERSION/macOS"
export VK_ICD_FILENAME="$VULKAN_SDK/share/vulkan/icd.d/MoltenVK_icd.json"
export VK_LAYER_PATH="$VULKAN_SDK/share/vulkan/explicit_layers.d"
export PATH="/usr/local/opt/python/libexec/bin:$VULKAN_SDK/bin:$PATH"
export ATH="/usr/local/bin:$PATH"
export PATH=/usr/local/bin:/usr/local/sbin:$PATH
PATH="/usr/local/Cellar/python@3.9/3.9.13/bin:${PATH}"
export SDKROOT="/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX12.1.sdk"
# Setting PATH for Python 3.9
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.9/bin:${PATH}"
export PATH
